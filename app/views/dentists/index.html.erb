<p style="color: green"><%= notice %></p>
<%= link_to 'Voltar', root_path, class: 'btn btn-primary' %>
<h1>Dentists</h1>

<div id="dentists">
  Busque dentistas pelo nome:
  <%= form_tag search_dentists_path, method: :get do %>
    <%= text_field_tag :query, params[:query] %>
    <%= submit_tag "Buscar" %>
  <% end %>
  <% @dentists.each do |dentist| %>
    <%= render dentist %>
    <p>
      <%= link_to "Show this dentist", dentist %>
    </p>
  <% end %>
</div>

<%= link_to "New dentist", new_dentist_path %>
